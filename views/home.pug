<style type="text/css">
nav {
position: fixed;
top: 100px;
left: 0;
}
nav a {
display: block;
text-align: center;
color: #FFF;
padding: 20px;
font-size: 40px;
margin-left: -200px;
width: 100px,  height: 100px;
transition-property: margin-left;
transition-duration: 0.2s;
transition-timing-function: ease;
}
nav a:hover {
color: #CFD8DC;
}
nav a:nth-child(1) {
transition-delay: 0s;
}
nav a:nth-child(2) {
  transition-delay: 0.2s;
}
nav a:nth-child(3) {
  transition-delay: 0.4s;
}
nav a:nth-child(4) {
  transition-delay: 0.6s;
}
#nav-toggle {
  width: 100px;
  height: 100px;
  border: none;
  background-color: rgba(0, 0, 0, 0);
  font: inherit;
  font-size: 40px;
  color:black;
  transition: -webkit-transform 0.2s ease;
  transition: transform 0.2s ease;
  transition: transform 0.2s ease, -webkit-transform 0.2s ease;
  padding: 0;
}
#nav-toggle:after {
  content: "\f0c9";
  font-family: 'FontAwesome';
}
#nav-toggle:focus {
  outline: none;

}
#nav-toggle:hover {
  color: #CFD8DC;
}
.nav-open nav a {
  margin-left: 0;
}
.nav-open #nav-toggle {
  -webkit-transform: rotate(-180deg);
          transform: rotate(-180deg);
}
.nav-open #nav-toggle:after {
  content: "\f00d";
}
/* Base Style */
@font-face {
  font-family: 'Raleway';
